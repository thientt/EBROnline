@model  IEnumerable<EBROnline.ViewModel.ListSubViewModel>

<div class="table-responsive">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th class="text-center">Stack Die Lot ID</th>
                <th class="text-center">Stack Die PMPN</th>
                <th class="text-center">Stack Die Qty</th>
                <th class="text-center">Stack Die Wafer Numbers</th>
                <th class="text-center">MES Lot Number</th>
                <th class="text-center">Wafer Qty</th>
                <th class="text-center">SAP Batch Lot Number</th>
                <th class="text-center">Build Quantity</th>
                <th class="text-center">Comment/Wafer</th>
                <th class="text-center"></th>
            </tr>
        </thead>
        <tbody class="bg-blue">
            @if (Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.StackDieId</td>
                        <td>@item.StackDiePMPN</td>
                        <td>@item.StackDieQty</td>
                        <td>@item.StackDieWaferNumbers</td>
                        <td>@item.LotNumber</td>
                        <td>@item.BatchLotNumber</td>
                        <td>@item.WaferQty</td>
                        <td>@item.BuildQuality</td>
                        <td>@item.Comment</td>
                        <td class="text-nowrap" style="width:100px;">
                            @if (item.IsAllow)
                            {
                                <button class="btn btn-link btn-min btn-edit" data-url="@Url.Action("Edit","SubDashboard",new{ id= item.Id})">Edit</button>
                                <span>|</span>
                                <button class="btn btn-link btn-min btn-delete" data-url="@Url.Action("Delete","SubDashboard",new{ id= item.Id})">Delete</button>
                            }
                            else
                            {
                                <span>Edit | Delete</span>
                            }
                        </td>
                    </tr>
                }
            }
        </tbody>

    </table>
</div>
